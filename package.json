    "prepare": "husky"
  },
  "dependencies": {
    "@types/js-cookie": "3.0.6",
    "@zuzjs/core": "^0.2.3",
    "@zuzjs/store": "^0.1.6",
    "@zuzjs/ui": "^0.9.6",
    "js-cookie": "^3.0.5",
    "next": "15.3.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.7",
    "@types/react-dom": "^19.1.6",
    "concurrently": "^9.1.2",
    "css-loader": "^7.1.2",
    "eslint": "^9.29.0",
    "eslint-config-next": "15.3.3",
    "sass": "^1.85.1",
    "style-loader": "^4.0.0",
    "typescript": "^5.8.3"
  }
}