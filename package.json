  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "css-loader": "^7.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "sass": "^1.85.1",
    "style-loader": "^4.0.0",
